{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "news_items",
  "info": {
    "name": "News-item"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "titre": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "lien_youtube": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    }
  }
}
